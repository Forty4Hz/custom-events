{
  "name": "highcharts-custom-events",
  "version": "3.0.8",
  "description": "Plugin that allows you to add extra events, like double / right click on each chart element, for Highcharts.",
  "main": "js/customEvents.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Forty4Hz/custom-events.git"
  },
  "types": "js/customEvents.d.ts",
  "keywords": [
    "highcharts",
    "highstock",
    "highmaps",
    "events",
    "highcharts-addon"
  ],
  "author": "Black Label <start@blacklabel.pl> (http://blacklabel.pl)",
  "license": "SEE LICENSE IN license.txt",
  "bugs": {
    "url": "https://github.com/Forty4Hz/custom-events/issues"
  },
  "homepage": "https://github.com/Forty4Hz/custom-events#readme",
  "files": [
    "js/customEvents.js",
    "js/customEvents.d.ts"
  ],
  "devDependencies": {
    "gulp": "^4.0.1",
    "gulp-eslint": "^2.0.0",
    "gulp-jsdoc3": "^2.0.0",
    "semantic-release": "^20.1.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^10.0.1",
    "@semantic-release/github": "^8.1.0",
    "@semantic-release/release-notes-generator": "^11.0.4",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1"
  },
  "dependencies": {
    "highcharts": ">=5.0.9"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
